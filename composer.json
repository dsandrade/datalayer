{
    "name": "1giba/datalayer",
    "description": "A simplest abstraction for eloquent models",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "repository",
      "datalayer",
      "models",
      "laravel",
      "lumen",
      "eloquent"
    ],
    "authors": [
        {
            "name": "Gilberto Junior",
            "email": "olamundo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OneGiba\\DataLayer\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestBase.php",
            "tests/TestRepository.php",
            "tests/TestPostgresRepository.php"
        ]
    },
    "require": {
        "php": "^7.1",
        "illuminate/database": "5.5.* || 5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0",
        "orchestral/testbench": "~3.5.0|~3.6.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
